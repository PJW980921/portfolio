{
  "version": 1,
  "projects": {
    "frontend": {
      "root": "/frontend",
      "builds": [
        {
          "src": "/frontend/package.json",
          "use": "@vercel/static-build",
          "config": { "distDir": "build" }
        }
      ],
      "routes": [
        {
          "src": "/(.*)",
          "dest": "/frontend/$1"
        }
      ]
    }
  }
}
